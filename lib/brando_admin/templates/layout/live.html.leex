<div id="admin" phx-hook="Brando.Admin">
  <%= if @socket_connected do %>
  <%= live_component @socket, BrandoAdmin.Components.PresenceEngine, id: "presence-engine", user_id: @current_user.id %>
  <main id="brando-main">
    <article class="content">
      <%= @inner_content %>
    </article>
  </main>
  <% end %>
</div>
