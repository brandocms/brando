<h1><%%= t!(@language, "actions.new") %></h1>
<%= "<%=" %> <%= alias %>Form.get_form(@language, type: :create, action: :create, params: [], values: @changeset.params, errors: @changeset.errors) <%= "%\>" %>
<div class="form-actions">
  <a href="<%= "<%=" %> Helpers.<%= admin_path %>_path(@conn, :index) <%= "%\>" %>" class="btn btn-default">
    <i class="fa fa-list fa-fw m-r-sm"></i> <%%= Brando.Admin.LayoutView.t!(@language, "global.back_to_index") %>
  </a>
</div>

<%= if villain do %>
<script type="text/javascript">
  $(document).ready(function() {
    v = new Villain.Editor({
      browseURL: '/admin/<%= no_plural %>/villain/bla/<%= singular %>/',
      uploadURL: '/admin/<%= no_plural %>/villain/last-opp/<%= singular %>/',
      textArea: 'textarea[name="<%= singular %>[data]"]'
    });
  });
</script>
<% end %>