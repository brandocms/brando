    <link rel="stylesheet" href="<%%= Routes.static_path(@conn, "/css/app.css") %>">
  </head>
  <%= body_tag @conn %>
    <%= cookie_law @conn, "Denne nettsiden bruker informasjonskapsler for statistikk og informasjon om besøkendes samhandling med nettsiden.<br>Klikk OK for å akseptere bruken av disse." %>
    <div class="fader" id="fader">
      <div class="loading-container">
        <div>
          <div class="display-3 mb-2">◣</div>
          <span class="blink">LOADING</span>
        </div>
      </div>
    </div>
    <script>var zf = document.getElementById("fader");zf.style.display = 'block';</script>
<!-- :content - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->
    <div class="backdrop"></div>
<!-- :nav - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->
    <%%= render <%= application_module %>Web.PageView, "_navigation.html", %{conn: @conn} %>
<!-- /nav - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->
    <%%= render @view_module, @view_template, assigns %>
    <section class="light">
      <section class="pt-sm pb-sm">
        <section class="container">
          <small>&copy; SITE NAME</small>
        </section>
      </section>
    </section>
<!-- /content - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->
    <script src="<%%= Routes.static_path(@conn, "/js/app.js") %>"></script>
    <%%= Brando.HTML.google_analytics("UA-XXXXX-X") %>
  </body>
</html>
