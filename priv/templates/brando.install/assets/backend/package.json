    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
