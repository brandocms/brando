{
  "name": "<%= application_name %>",
  "description": "<%= application_name %> Frontend",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "lint:js": "standard --verbose | snazzy",
    "build": "webpack --mode=production --config webpack.prod.js",
    "build:cdn": "BRANDO_CDN=<%= application_name %> NODE_ENV=production webpack --mode=production --config ./webpack.config.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "test": "cypress open",
    "move_srcmaps": "move-file ../../priv/static/js/app.js.map ../../priv/static/js/__srcmaps/app.js.map && move-file ../../priv/static/js/app.legacy.js.map ../../priv/static/js/__srcmaps/app.legacy.js.map && move-file ../../priv/static/css/app.css.map ../../priv/static/css/__srcmaps/app.css.map"
  },
  "browserslist": {
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "ie > 10",
      "safari >= 9",
      "not dead"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "standard": {
    "globals": [
      "__VERSION__",
      "__ENV__",
      "__VSN_HASH__",
      "__APP_NAME__"
    ]
  },
  "private": true,
  "author": {
    "name": "Univers",
    "email": "mail@univers.agency",
    "url": "http://univers.agency"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "@sentry/browser": "^5.18.1",
    "@univers-agency/jupiter": "file:.yalc/@univers-agency/jupiter",
    "@univers-agency/panama": "file:.yalc/@univers-agency/panama",
    "core-js": "3.6.5",
    "custom-event-polyfill": "^1.0.7",
    "flickity": "^2.2.1",
    "intersection-observer": "^0.11.0",
    "picturefill": "^3.0.3",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@univers-agency/europacss": "file:.yalc/@univers-agency/europacss",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
