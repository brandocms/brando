    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^6.0.0",
    "git-rev-sync": "^2.0.0",
    "glob-all": "^3.2.1",
    "html-webpack-plugin": "^4.2.0",
