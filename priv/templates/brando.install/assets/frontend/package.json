{
  "name": "<%= application_name %>",
  "description": "<%= application_name %> FRONTEND",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "lint:js": "standard --verbose | snazzy",
    "dev": "webpack --mode=development --debug --watch --colors",
    "build": "yarn build:legacy && yarn build:modern",
    "build:cdn": "yarn build:legacy:cdn && yarn build:modern:cdn",
    "build:legacy": "NODE_ENV=production BROWSERSLIST_ENV=legacy webpack --mode=production",
    "build:modern": "NODE_ENV=production BROWSERSLIST_ENV=modern webpack --mode=production",
    "build:legacy:cdn": "UNIVERS_CDN=<%= application_name %> NODE_ENV=production BROWSERSLIST_ENV=legacy webpack --mode=production",
    "build:modern:cdn": "UNIVERS_CDN=<%= application_name %> NODE_ENV=production BROWSERSLIST_ENV=modern webpack --mode=production",
    "watch": "webpack-dev-server --mode development --hot --watch-stdin --optimize-max-chunks 1",
    "test": "cypress open"
  },
  "standard": {
    "globals": [
      "VERSION",
      "ENV"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "core-js": "3.1.4",
    "custom-event-polyfill": "^1.0.7",
    "cypress": "^3.4.0",
    "enquire-js": "^0.2.1",
    "eslint-plugin-cypress": "^2.2.1",
    "fitty": "^2.2.6",
    "flickity": "^2.2.1",
    "flickity-imagesloaded": "^2.0.0",
    "font-awesome": "^4.7.0",
    "gsap": "^2.1.3",
    "hamburgers": "^1.1.3",
    "hammerjs": "^2.0.8",
    "include-media": "^1.4.9",
    "intersection-observer": "^0.7.0",
    "io": "twined/io",
    "jupiter": "twined/jupiter",
    "macy": "^2.5.0",
    "picturefill": "^3.0.3",
    "regenerator-runtime": "^0.13.2",
    "scroll-into-view-if-needed": "^2.2.20"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint-plugin-compat": "^3.2.0",
    "extract-css-chunks-webpack-plugin": "^4.5.6",
    "file-loader": "^4.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.1",
    "snazzy": "^8.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "standard": "^13.0.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
